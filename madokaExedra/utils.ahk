#Requires AutoHotkey >=2.0-a

#Include "%A_ScriptDir%/../utils.ahk"

confirm() {
  pressKey("Enter")
}

goBack() {
  pressKey("Esc")
}

goLeft() {
  pressKey("a")
}

goRight() {
  pressKey("d")
}

skip() {
  pressKey("y")
}

accessMagiaBox() {
  pressKey("f")
}

accessMissions() {
  pressKey("b")
}

accessGiftbox() {
  pressKey("n")
}
